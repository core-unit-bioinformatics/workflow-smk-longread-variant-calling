# EditorConfig is awesome: http://EditorConfig.org

# top-most EditorConfig file
root = true

[*]
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true
charset = utf-8
indent_style = space
indent_size = 4

[*.{tsv,tab}]
indent_style = tab

[*.{yml,yaml,json,toml,tml}]
indent_style = space
indent_size = 2
